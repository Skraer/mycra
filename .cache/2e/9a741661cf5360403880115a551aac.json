  fs.writeFileSync(certificatePath, pems.private + pems.cert, {\n      encoding: 'utf8',\n    });\n  }\n\n  return fs.readFileSync(certificatePath);\n}\n\nmodule.exports = getCertificate;\n"},"lineCount":null}},"error":null,"hash":"d9ee006b6833993fa4404cc3bb42fd77","cacheData":{"env":{}}}